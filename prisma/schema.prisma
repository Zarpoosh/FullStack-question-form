// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL2")
}

model Survey {
  id        Int      @id @default(autoincrement())
  firstName String
  lastName  String
  gender    String
  answers   Json?
  createdAt DateTime @default(now())
}

